<div class="content-blurb shadow">
	<h3 class="content-header">
	<%= @device_type.name -%>
	</h3>
	<div class="subnav-group">
		<%= link_to 'Edit', edit_device_type_path(@device_type) %> |
		<%= link_to 'Back', device_types_path %>
	</div>
	<hr>
	<table class="table table-striped table-bordered table-hover table-condensed">
		<tr>
			<td>ID:</td>
			<td><%= @device_type.id -%></td>
		</tr>
		<tr>
			<td>Name:</td>
			<td><%= @device_type.name -%></td>
		</tr>
		<tr>
			<td>Created At:</td>
			<td><%= @device_type.created_at -%></td>
		</tr>
		<tr>
			<td>Updated At:</td>
			<td><%= @device_type.updated_at -%></td>
		</tr>
	</table>
	<br/>
</div>

<div class="span10 content-blurb shadow">
	<h3 class="content-header">Editing 
		<%= @device_type.name -%>
	</h3>
	<%= link_to 'Show', @device_type %> |
	<%= link_to 'Back', device_types_path %>
	<hr>
	<%= form_for(@device_type) do |f| %>
		<label><%= f.label :name -%></label>
		<%= f.text_field :name -%>
		<%= f.submit -%>
	<%- end -%>
</div>

<div class="content-blurb shadow">
	<h3 class="content-header"> 
	   Listing DNS Device Associations
	</h3>
	<hr>
	<table id="dns-index-table" class="table-striped table-borded table-hover table-condensed">
	<thead>
  	<tr>
  	   <th>Device Name</th>
  	   <th>IP Identification</th>
  	   <th>Created At</th>
  	   <th>Updated At</th>
 	   <th>Options</th>
  	</tr>
	</thead>

	<% @dns_device_assocs.each do |dns_device_assoc| %>
  	<tr>
   	  <td> <%= dns_device_assoc.name %></td>
		  <td><%= dns_device_assoc.ip_id %></td>
		  <td><%= dns_device_assoc.created_at %></td>
		  <td><%= dns_device_assoc.updated_at %> </td>
		  <td>
			  <%= link_to 'Show', dns_device_assoc %>
 	   	  <%= link_to 'Edit', edit_dns_device_assoc_path(dns_device_assoc) %>
 	   	  <%= link_to 'Destroy', dns_device_assoc, method: :delete, data: { confirm: 'Are you sure?' } %>
			</td>
  	</tr>
	<% end%>
	</table>
	<br/><br/>
	<%= link_to 'New Dns Device', new_dns_device_assoc_path, :class => "btn btn-primary" %>
	<br/>
</div>
